#include <stdio.h>
int main()
{
    char a = 30;
    char b = 40;
    char c = 10;
    char d = (a * b) / c; //output is 120.because it follows the bodmas rule.
    printf ("the output:%d ", d);    //in bodmas first divide and multiplication and addition and subtraction.
                          // and it has the brackets and flower brackets and square brackets.
           
}
2)
#include<stdio.h>
int main()
{
    int a,b=10;
    a = -b--;
    printf("a=%d\n",a); // first it is updated because it is a post increament .first it is not change the value but after the value change. 
    printf("b=%d\n",b);
}
